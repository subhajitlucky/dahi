# Environment Variables Template
# Copy this file to .env for local development
# Note: Never commit .env files to version control

# Application Configuration
# VITE_APP_TITLE=Super Dahi
# VITE_APP_DESCRIPTION=Mindful Yogurt Fermentation

# API Configuration (if needed in future)
# VITE_API_BASE_URL=http://localhost:3000
# VITE_API_KEY=your_api_key_here

# Feature Flags (if needed in future)
# VITE_ENABLE_ANALYTICS=false
# VITE_ENABLE_COMMENTS=false

# External Services (if needed in future)
# VITE_GOOGLE_ANALYTICS_ID=
# VITE_SENTRY_DSN=

# Contact Information
# VITE_CONTACT_EMAIL=lab@superdahi.co

# Social Media Links (if needed in future)
# VITE_TWITTER_URL=
# VITE_INSTAGRAM_URL=
# VITE_FACEBOOK_URL=

# Development Settings
# VITE_DEBUG_MODE=false
# VITE_LOG_LEVEL=info
